<form action="/tennis_player" method="POST">
  <p>
    <label>First Name</label>
    <input type="text" name="first_name"/>
  </p>
  <p>
    <label>Last Name</label>
    <input type="text" name="last_name"/>
  </p>
  <p>
    <label>Age</label>
    <input type="text" name="age"/>
  </p>
  <p>
    <label>Ranking</label>
    <input type="text" name="ranking"/>
